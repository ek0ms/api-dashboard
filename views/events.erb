<div class="small-6 small-offset-3 columns">
  <div class="callout primary">
    <h3>Events</h3>
    <p>"<%= @geolocation.city %>, <%= @geolocation.state %>"</p>
  </div>

  <div class="callout primary">
    <h3>Check these out!</h3>
    <p>
